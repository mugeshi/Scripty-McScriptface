// array method
const items = [
    {name:'Bike', price:100},
    {name:'TV', price:200},
    {name:'Album', price:10},
    {name:'Book', price:5},
    {name:'phone', price:500},
    {name:'computer', price:1000},
    {name:'keybord', price:25}

]

//1. Filter method
const filterItems = items.filter((item) => {
    return item.price <= 100
})

console.log(filterItems)